create_table("votes") {
	t.Column("id", "integer", {"primary": true})
	t.Column("post_id", "uuid", {})
	t.Column("user_id", "uuid", {})
    t.ForeignKey("post_id", {"posts": ["id"]}, {"on_delete": "cascade"})
    t.ForeignKey("user_id", {"users": ["id"]}, {"on_delete": "cascade"})
    t.Timestamps()
}
